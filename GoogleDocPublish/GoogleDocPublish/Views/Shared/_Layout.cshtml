<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="google-site-verification" content="uiPAh3jcwr90Wr6gATYZZWYbd5RgYUZdBZOblj5YLG8" />
    <title>@ViewData["Title"]</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment> 

    @* <!--Global site tag (gtag.js) - Google Analytics --> *@
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-112884116-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-112884116-1');
    </script>
    @*     <!--END of Google Analytics -- > *@

    @* Structured data for search oprimisation*@
    <script type="application/ld+json">
        {
        "@@context": "http://schema.org",
        "@@type": "Person",
        "name": "Denis Palnitsky",
        "url": "http://denispalnitsky.com",
        "sameAs": [
        "http://www.facebook.com/denis.palnitsky",
        "http://www.linkedin.com/in/denis-palnitsky",
        "https://github.com/DenisPalnitsky",
        "https://github.com/DenisPalnitsky",
        "https://stackoverflow.com/users/238123/denis-palnitsky"
        ],
        "jobTitle": "Senior Software Developer / Architect",
        "telephone": "+48 576 192 323",
        "email": "palnitsky@gmail.com"
        }
    </script>

</head>
        <body>
            @*<nav class="navbar navbar-inverse navbar-fixed-top">
                <div class="container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
